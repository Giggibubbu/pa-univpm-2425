{
  "name": "progetto_progavanzata2425",
  "version": "1.0.0",
  "description": "Progetto Programmazione Avanzata AA 24/25",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "start": "node dist/app.js",
    "build": "tsc",
    "dev": "nodemon --watch dist dist/app.js",
    "olddev1": "tsx watch src/app.ts",
    "olddev": "ts-node-dev --respawn --transpile-only --exit-child src/app.ts",
    "buildModels": "sequelize-auto --sg=true -l ts -o ./src/models -h localhost -d pa2425 -u postgres -x postgres -p 5432 -e postgres --useDefine --cm=p --cp=c --cf=p --noAlias",
    "buildNavReqModel": "sequelize-auto --sg -l ts -o ./src/models -h localhost -d pa2425 -u postgres -x postgres -p 5432 -e postgres -t navigation_requests",
    "buildNoNavModel": "sequelize-auto --sg -l ts -o ./src/models -h localhost -d pa2425 -u postgres -x postgres -p 5432 -e postgres -t no_navigation_zones"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giggibubbu/progetto_progavanzata2425.git"
  },
  "author": "Lorenzo Olivieri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Giggibubbu/progetto_progavanzata2425/issues"
  },
  "homepage": "https://github.com/Giggibubbu/progetto_progavanzata2425#readme",
  "dependencies": {
    "@turf/turf": "^7.3.3",
    "@types/bcrypt": "^6.0.0",
    "@types/geojson": "^7946.0.16",
    "bcrypt": "^6.0.0",
    "express": "^5.1.0",
    "express-validator": "^7.3.1",
    "geojson": "^0.5.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.17.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-auto": "^0.8.8",
    "xmlbuilder2": "^4.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.4",
    "@types/typescript": "^0.4.29",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.4.6",
    "tsc": "^2.0.4",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.50.0"
  }
}
